set number
colo jmoney
syntax on

set showmode
set nowrap
set tabstop=4
set smarttab
set tags=tags
set softtabstop=4
set expandtab
set shiftwidth=4
set shiftround
set backspace=indent,eol,start
set autoindent
set copyindent
set ignorecase
set smartcase

set visualbell
set noerrorbells
set autowrite
set mouse=a

nmap <C-v> :vertical resize +5

nmap <C-b> :NERDTreeToggle<cr>
